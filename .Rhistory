# fonksiyonu oluşturalım
f_sd <- function(x) {
result <- sqrt(sum((x - mean(x))^2) / (length(x) - 1))
return(result)
}
set.seed(123) # Üreteç sabitlenir
x1 <- rnorm(1000, 0, 1.0) # normal dağılımdan 1000 adet sayı üretilir
f_sd(x1)
